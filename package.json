{
  "name": "@caasi/hooks",
  "version": "1.5.6",
  "description": "Some useful React Hooks",
  "main": "dist/index.js",
  "scripts": {
    "build:clean": "rimraf dist/*",
    "build:babel": "babel --ignore 'src/**/__tests__/' src --out-dir dist",
    "build": "npm run build:clean && npm run build:babel",
    "prettier": "npx prettier --write './src/**/*.js'",
    "cypress:open": "cypress open",
    "test:jest": "jest",
    "test:cypress": "cypress run",
    "test": "npm run test:jest && npm run test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/caasi/hooks.git"
  },
  "keywords": [
    "React",
    "React Hooks"
  ],
  "author": "caasi Huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caasi/hooks/issues"
  },
  "homepage": "https://github.com/caasi/hooks#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@testing-library/react-hooks": "^3.2.1",
    "blob": "0.0.5",
    "cypress": "^3.7.0",
    "cypress-react-unit-test": "^2.4.3",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
}
